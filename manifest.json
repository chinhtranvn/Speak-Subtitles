{
   "action": {
      "default_icon": "assets/icon/Eva-Wall-E-128.png",
      "default_popup": "popup.html",
      "default_title": "Speak Subtitles for YouTube"
   },
   "author": "Aulon",
    "background": {
       "service_worker": "sw.js"
    },
    "content_scripts": [ {
       "js": [ "content.bundle.js", "googleTtsShim.js", "webpage.bundle.js", "ga.bundle.js" ],
       "matches": [ "https://www.youtube.com/*" ],
       "run_at": "document_idle"
    } ],
   "default_locale": "en",
   "description": "__MSG_extension_description__",
   "externally_connectable": {
      "matches": [ "https://www.youtube.com/*" ]
   },
    "host_permissions": [ "https://www.youtube.com/*", "https://texttospeech.googleapis.com/*" ],
   "icons": {
      "128": "assets/icon/Eva-Wall-E-128.png",
      "512": "assets/icon/Eva-Wall-E-512.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "88.0",
   "name": "__MSG_extension_name__",
   "options_page": "don.html",
   "options_ui": {
      "open_in_tab": true,
      "page": "don.html"
   },
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.41"
}
